<Window x:Class="ZZZIFOX.WPFUI.NewCopyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZZZIFOX.WPFUI"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="复制定义" Height="188" Width="310" Closed="Window_Closed" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid>
        <Label Content="规则:个数1*距离1,个数2*距离2......" HorizontalAlignment="Center" Margin="0,7,0,0" VerticalAlignment="Top"/>
        <Label Content="复制规则" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="68,41,0,0" TextWrapping="Wrap" Text="{Binding TextString}" VerticalAlignment="Top" Width="221"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="48,109,0,0" VerticalAlignment="Top" Width="48" Click="Button_Click"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="191,109,0,0" VerticalAlignment="Top" Width="52" Click="Button_Click_1"/>
    </Grid>



</Window>
